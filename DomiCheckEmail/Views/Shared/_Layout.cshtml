<!DOCTYPE html>
<html lang="es">
<head>
    <title>@ViewBag.Title</title>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js" type="text/javascript"></script>
    <script src="http://html5shim.googlecode.com/svn/trunk/html5.js" type="text/javascript"></script>
    <link href="../../Content/bootstrap/css/bootstrap.css" rel="stylesheet"/>
    <link href="../../Content/bootstrap/css/bootstrap-responsive.min.css" rel="stylesheet" /> 
    <script src="../../Content/bootstrap/js/bootstrap.js" type="text/javascript"></script>
     <script src="../../Content/bootstrap/js/bootstrap-modal.js" type="text/javascript"></script>
    <script src="../../Scripts/jquery.unobtrusive-ajax.js" type="text/javascript"></script>
    <script src="../../Scripts/modernizr-1.7.min.js" type="text/javascript"></script>
</head>
<body>    
    <div>
        <div id="header">
            
        </div>
        <div id="main">
            <div align="center">
                <div id="Contenedor" class="well" style="width:80%;">
                    <div id="TablaSuperior" style="text-align:right; margin-right: 33%">
                       
                    </div>
                    <div style="text-align: center;" ><img src="http://www.domitienda.com/Imagenes/logocabecera.png" width="400px" alt="Domitienda" /></div>                                                                  
                    <table id="PaginaPrincipal" width="100%" style="margin-top:-3px;" cellpadding="0" cellspacing="0">
                        <tbody>
                            <tr>
                                <td style="width: 98%;">
                                    @RenderBody()
                                </td>                                
                            </tr>
                        </tbody>
                    </table>                     
                </div>
            </div>
        </div>        
    </div>
</body>
</html>